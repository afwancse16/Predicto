<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_college</servlet-name>
        <servlet-class>add_college</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>get_college</servlet-name>
        <servlet-class>get_college</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_cutoff</servlet-name>
        <servlet-class>update_cutoff</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_college</servlet-name>
        <url-pattern>/add_college</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>get_college</servlet-name>
        <url-pattern>/get_college</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_cutoff</servlet-name>
        <url-pattern>/update_cutoff</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
